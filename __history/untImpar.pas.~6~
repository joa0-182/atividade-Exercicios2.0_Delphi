{$R *.dfm}

procedure TfrmImpar.btnGerarClick(Sender: TObject);
var num1, num2, resImpar, i, ii: integer;
begin

      ii := 1;

      for i := 100 to 200 do
        begin
           memResultado.Lines.Add('° Valor =' + IntToStr(ii));
           ii := ii + 2;
        end;






